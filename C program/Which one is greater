#include<stdio.h>
main()
{
    int a,b,c;
    printf("enter value for a=");
    scanf("%d",&a);
    printf("enter value for b=");
    scanf("%d",&b);
    printf("enter value for c=");
    scanf("%d",&c);
    if(a>b && b<c)
    {
        printf("\n a is greater");
    }
    if(b>a && b>c)
    {
        printf("\n b is greater");
    }
    else
    {
        printf("\n c is greater");
    }
}
