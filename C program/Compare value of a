#include<stdio.h>
main()
{
    int a;
    printf("enter value for a=");
    scanf("%d",&a);
    if(a==20)
    {
        printf("\n a is equal to 20");
    }
    if(a>=20)
    {
        printf("\n a is greater than or equal to 20");
    }
    if(a<=20)
    {
        printf("\n a is less than or equal to 20");
    }
}
