#include<stdio.h>
main()
{
    int sp,cp,profit,loss;
    printf("\nEnter value of cp");
    scanf("%d",&cp);
    printf("\n Enter value of sp");
    scanf("%d",&sp);
    profit=sp-cp;
    loss=cp-sp;
    if(profit>loss)
    {
        printf("profit=%d",profit);
    }
    else
    {
        printf("loss=%d",loss);
    }
}
